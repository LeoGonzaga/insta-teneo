{
  "manifest_version": 2,
  "name": "Insta Teneo",
  "description": "Acelere os audios do Insta",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/direct/inbox/*","*://www.instagram.com/"],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "128": "assets/icon128.png"
  }
}
